  <div class="col-md-8">
    <% if !signed_in? && !current_user%>
    <% deny_access %>
    <% end -%>
    <h1>Avatar</h1>
    <div class="row photos_row">
      <% @photos.each do |photo| %>
        <% if photo.avatar == "1" %>
          <div class="photo">
            <h4><%= link_to 'Change avatar', edit_photo_path(id: photo.id) %></h4>
            <div><%= link_to image_tag(photo.picture_url, :width => 150), photo_path(id: photo.id) %></div>
            <p><%= photo.name %></p>
          </div>
        <% end -%>
      <% end -%>
    </div>
    <h1>Photos</h1>
    <h4><%= link_to 'Add Photo', new_photo_path %></h4>
    <div class="row photos_row">
      <% @photos.each do |photo| %>
      <div class="photo">
        <% if photo.avatar != "1" %>
          <div><%= link_to image_tag(photo.picture_url, :width => 150), photo_path(id: photo.id) %></div>
          <p><%= photo.name %></p>
        <% end -%>
      </div>
      <% end %>
    </div>
    <br>



  </div>
  <div class="col-md-2"></div>
</div>
